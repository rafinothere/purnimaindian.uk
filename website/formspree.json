{
    "forms": {
      "reservation": {
        "name": "Reservation Form",
        "actions": [
          {
            "type": "email",
            "to": "your-email@gmail.com",             "subject": "New Reservation",
            "fields": [
              {
                "name": "name",
                "label": "Name"
              },
              {
                "name": "email",
                "label": "Email"
              },
              {
                "name": "phone",
                "label": "Phone"
              },
              {
                "name": "guests",
                "label": "Number of Guests"
              },
              {
                "name": "date",
                "label": "Date"
              },
              {
                "name": "time",
                "label": "Time"
              }
            ]
          },
          {
            "type": "email",
            "to": "{{email}}",
            "subject": "Reservation Confirmation",
            "body": "Thank you for your reservation! We have received your booking details."
          }
        ]
      }
    }
  }
  